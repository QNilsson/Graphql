generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model recipe {
  id    Int    
  title String  @unique
  readyInMinutes  Int
  servings Int
  sourceUrl String?
  image String
}

model list{
  id Int @id @default(autoincrement())
  uid String
  // recipeList Int[]
}
